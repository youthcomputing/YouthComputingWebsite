<html>

<head>
	<% include ../partials/head %>
</head>

<body>

	<div class="gtco-loader"></div>

	<div id="page">
		<% include ../partials/nav %>

		<header id="gtco-header about" class="gtco-cover gtco-cover-xs" role="banner"
			style="background-image:url(images/img_bg_1.jpg);">
			<div class="overlay"></div>
			<div class="gtco-container">
				<div class="row">
					<div class="col-md-8 col-md-offset-2 text-center">
						<div class="display-t">
							<div class="display-tc">
								<h1 class="animate-box" data-animate-effect="fadeInUp">Get In Touch</h1>
								<h2 class="animate-box" data-animate-effect="fadeInUp">Feel free to contact us at any
									time. We'd love to hear from you!</h2>
							</div>
						</div>
					</div>
				</div>
			</div>
		</header>

		<div class="gtco-section">
			<div class="gtco-container">
				<div class="row row-pb-md">
					<div class="col-md-6 animate-box">
						<h3>Get In Touch</h3>
						<form action="https://formspree.io/youthcomputingstem@gmail.com" method="POST">
							<div class="row form-group">
								<div class="col-md-12">
									<label class="sr-only" for="name">Name</label>
									<input type="text" id="name" name="name" class="form-control"
										placeholder="Your firstname">
								</div>

							</div>

							<div class="row form-group">
								<div class="col-md-12">
									<label class="sr-only" for="email">Email</label>
									<input type="text" id="email" name="email" class="form-control"
										placeholder="Your email address">
								</div>
							</div>

							<div class="row form-group">
								<div class="col-md-12">
									<label class="sr-only" for="subject">Subject</label>
									<input type="text" id="subject" name="subject" class="form-control"
										placeholder="Your subject of this message">
								</div>
							</div>

							<div class="row form-group">
								<div class="col-md-12">
									<label class="sr-only" for="message">Message</label>
									<textarea name="message" id="message" cols="30" rows="10" class="form-control"
										placeholder="Write us something"></textarea>
								</div>
							</div>
							<div class="form-group">
								<button type="submit" class="btn btn-primary btn-lg">Send Message</button>
							</div>

						</form>
					</div>
					<div class="col-md-5 col-md-push-1 animate-box">

						<div class="gtco-contact-info">
							<h3>Contact Information</h3>
							<ul>
								<li class="address">Fort McMurray, AB</li>
								<li class="phone"><a href="tel:17803810421">+1 780 381 0421</a></li>
								<li class="email"><a href="mailto:info@youthcomputing.ca">info@youthcomputing.ca</a>
								</li>
								<li class="url"><a href="https://youthcomputing.ca">youthcomputing.ca</a></li>
							</ul>
						</div>


					</div>
				</div>
			</div>

		</div>

		<% include ../partials/footer %>

</body>

</html>